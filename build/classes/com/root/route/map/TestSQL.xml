<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test">

	<select id="selectTestList" resultType="hashmap">
       SELECT NO as "no",
		   NAME as "name",
		   DEL_YN as "delYn"
		FROM TEST
    </select>
    
</mapper>